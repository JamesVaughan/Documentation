{



  "metadata": [
    {
      "src": [
        {
          "files": [
            "**.csproj"
          ],
          "exclude": [ "**/4Step/**", "**/obj/**" ],
		      "src": "../../XTMF/Code/" 
        }
      ],
      "dest": "api",
      "disableGitFeatures": false,
      "_appLogoPath":"images/tmg.png",
      "filter": "filterConfig.yml"
    }
  ],
  "build": {
  "globalMetadata": {
    "_appTitle": "Travel Modelling Group Documentation",
    "_gitContribute": {
      "repo": "https://github.com/TravelModellingGroup",
      "branch": "dev",
      "apiSpecFolder": "docs-ref-overwrite"
    },
    "_appLogoPath":"images/tmg.png",
    "_appFooter": "Â©2018, Travel Modelling Group",
    "_navRel": "toc.html",
    "_enableSearch": "true"
  },
    "content": [
      {
        "files": ["*.md", "*.yml"]

      },
      {
        "files": [
          "articles/**.md",
          "articles/**/toc.yml",
          "tmgtoolbox/**.md",
          "tmgtoolbox/**/toc.yml",
          "tutorial/**.md",
          "tutorial/**/toc.yml",
          "gtamodel/**.md",
          "gtamodel/**/toc.yml",
          "toc.yml",
          "*.md"
        ],
        "src": "v1.4",
        "dest":"v1.4",
        "navRoot": "v1.4"
      },
      {
        "files": [
          "articles/**.md",
          "articles/**/toc.yml",
          "tmgtoolbox/**.md",
          "tmgtoolbox/**/toc.yml",
          "tutorial/**.md",
          "tutorial/**/toc.yml",
          "gtamodel/**.md",
          "gtamodel/**/toc.yml",
          "toc.yml",
          "*.md"
        ],
        "src": "v1.5",
        "dest":"v1.5",
        "navRoot": "v1.5"
      }
    ],
    "resource": [
      {
        "files": [
          "images/**",
          "**/images/**"
        ]
      }
    ],
    "overwrite": [
      {
        "files": [
          "apidoc/**.md"
        ],
        "exclude": [
          "obj/**",
          "_site/**"
        ]
      }
    ],
    "dest": "_site",
    "globalMetadataFiles": [],
    "fileMetadataFiles": [],
    "template": [
      "default",
      "template"
      
    ],
    "postProcessors": ["ExtractSearchIndex"],
    "markdownEngineName": "markdig",
    "noLangKeyword": false,
    "keepFileLink": false,
    "cleanupCacheHistory": false,
    "disableGitFeatures": false
  }
}